<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.CashRegister.SupplierEntity">
        <table name="SUPPLIER" schema="SBD_ST_PS11_4" catalog=""/>
        <attributes>
            <id name="nip">
                <column name="NIP" precision="10"/>
            </id>
            <basic name="countryorigin">
                <column name="COUNTRYORIGIN" nullable="false" length="56"/>
            </basic>
            <basic name="name">
                <column name="NAME"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
