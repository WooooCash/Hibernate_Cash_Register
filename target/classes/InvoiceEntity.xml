<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.CashRegister.InvoiceEntity">
        <table name="INVOICE" schema="SBD_ST_PS11_4" catalog=""/>
        <attributes>
            <id name="invoiceId">
                <column name="INVOICE_ID" precision="2147483646"/>
            </id>
            <basic name="nip">
                <column name="NIP" precision="10"/>
            </basic>
            <basic name="netprice">
                <column name="NETPRICE" precision="2"/>
            </basic>
            <basic name="taxamount">
                <column name="TAXAMOUNT" precision="2"/>
            </basic>
            <basic name="firstname">
                <column name="FIRSTNAME" length="100"/>
            </basic>
            <basic name="lastname">
                <column name="LASTNAME" length="100"/>
            </basic>
            <basic name="address">
                <column name="ADDRESS" nullable="false"/>
            </basic>
